{
    "name": "halloverden/symfony-brreg-http-client",
    "description": "Provides a Symfony http client for the brreg (Brønnøysundregistrene) api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Giannetti",
            "email": "marco@halloverden.no"
        }
    ],
    "keywords": [
        "http",
        "brreg",
        "client",
        "api",
        "register",
        "REST"
    ],
    "support": {
        "issues": "https://github.com/halloverden/symfony-brreg-http-client/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "symfony/http-client": "^6.4|^7.2",
        "halloverden/symfony-http-exceptions": "^2.0",
        "jms/serializer-bundle": "^5.0",
        "doctrine/collections": "^1.6|^2.2"
    },
    "autoload": {
        "psr-4": {
            "HalloVerden\\BrregHttpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HalloVerden\\BrregHttpClient\\Tests": "tests"
        }
    },
    "require-dev": {
        "symfony/validator": "^5.4|^6.1",
        "phpunit/phpunit": "^12.0"
    }
}
